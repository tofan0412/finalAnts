<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">



	<resultMap id="MemberVo" type="ants.com.member.model.MemberVo">
		<result column="mem_id" property="memId"/>
		<result column="mem_name" property="memName"/>
		<result column="mem_pass" property="memPass"/>
		<result column="mem_tel" property="memTel"/>	
		<result column="mem_filepath" property="memFilepath"/>	
		<result column="mem_filename" property="memFilename"/>	
		<result column="mem_alert" property="memAlert"/>	
		<result column="del" property="del"/>	
		<result column="mem_type" property="memType"/>	
	</resultMap>


	
	<select id="getMember" parameterType="String" resultMap="MemberVo">
		SELECT * FROM MEMBER
		 WHERE MEM_ID = #{memId}
	</select>
	
	<insert id="insertMember" parameterType="MemberVo">
    	INSERT INTO MEMBER VALUES(#{memId}, #{memName}, #{memPass}, #{memTel}, #{memFilepath}, #{memFilename}, #{memAlert}, #{del}, #{memType})
   	</insert>

</mapper>